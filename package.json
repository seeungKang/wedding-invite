{
  "name": "wedding-invite",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "homepage": "https://seeungkang.github.io/wedding-invite/",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:lang": "node scripts/check-language.mjs",
    "check:size": "node scripts/check-bundle-size.mjs",
    "optimize:images": "node scripts/optimize-images.mjs",
    "ci": "npm run lint && npm run typecheck && npm run check:lang && npm run build && npm run check:size",
    "preview": "vite preview",
    "deploy": "npm run ci && gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.5.0",
    "gh-pages": "^6.1.1",
    "globals": "^17.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.56.0",
    "vite": "^5.4.2"
  }
}
